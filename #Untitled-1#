	 employees = []  # Initialise an empty array
	 for i in range(len(companies_dict)):
		  if companies_dict[i]['company'] == name:
				# Pass the i and 'employees' to a function
				employees = getEmployees(i, employees)
				if len(employees):
					 return jsonify(employees)
				else:
					 return jsonify("Company has no employees")
	 return jsonify("Company is not found")

